{
  "navigationBarBackgroundColor": {
    "prefix": "navigationBarBackgroundColor",
    "body": ["\"navigationBarTextStyle\": \"${1:white}\""],
    "description": "导航栏标题颜色，仅支持 black/white。"
  },
  "navigationBarTitleText": {
    "prefix": "navigationBarTitleText",
    "body": ["\"navigationBarTitleText\": \"${1}\""],
    "description": "导航栏标题文字内容。"
  },
  "backgroundColor": {
    "prefix": "backgroundColor",
    "body": ["\"backgroundColor\": \"${1:#000000}\""],
    "description": "tab 的背景色。"
  },
  "backgroundTextStyle": {
    "prefix": "backgroundTextStyle",
    "body": ["\"backgroundTextStyle\": \"${1:dark}\""],
    "description": "下拉背景字体、loading 图的样式，仅支持 dark/light。"
  },
  "enablePullDownRefresh": {
    "prefix": "enablePullDownRefresh",
    "body": ["\"enablePullDownRefresh\": ${1:false}"],
    "description": "是否开启下拉刷新。"
  },
  "color": {
    "prefix": "color",
    "body": ["\"color\": \"${1:#000000}\""],
    "description": "tab 上的文字默认颜色。"
  },
  "selectedColor": {
    "prefix": "selectedColor",
    "body": ["\"selectedColor\": \"${1:#000000}\""],
    "description": "tab 上的文字选中时的颜色。"
  },
  "borderStyle": {
    "prefix": "borderStyle",
    "body": ["\"borderStyle\": \"${1:black}\""],
    "description": "tabbar上边框的颜色， 仅支持 black/white 。"
  },
  "tabBar": {
    "prefix": "tabBar",
    "body": [
      "\"tabBar\": {",
      "\t\"list\": [{",
      "\t\t\"pagePath\": \"${1:pagePath}\",",
      "\t\t\"text\": \"${2:text}\",",
      "\t\t\"iconPath\": \"${3:iconPath}\",",
      "\t\t\"selectedIconPath\": \"${4:selectedIconPath}\"",
      "\t}]",
      "}"
    ],
    "description": "tab 的列表最少2个、最多5个 tab"
  },
  "pages": {
    "prefix": "pages",
    "body": ["\"pages\": [", "\t${1:Entrance}", "],"],
    "description": "设置页面路径。"
  },
  "window": {
    "prefix": "window",
    "body": [
      "\"window\": {",
      "\t\"navigationBarBackgroundColor\": \"${1:#ffffff}\",",
      "\t\"navigationBarTextStyle\": \"${2:black}\",",
      "\t\"navigationBarTitleText\": \"${3:小程序}\",",
      "\t\"backgroundColor\": \"${4:#eeeeee}\",",
      "\t\"backgroundTextStyle\": \"${5:light}\",",
      "\t\"enablePullDownRefresh\": ${6:false}",
      "}"
    ],
    "description": "设置默认页面的窗口表现。"
  },
  "networkTimeout": {
    "prefix": "networkTimeout",
    "body": [
      "\"networkTimeout\": {",
      "\t\"request\": ${1:20000},",
      "\t\"connectSocket\": ${2:20000},",
      "\t\"uploadFile\": ${3:20000},",
      "\t\"downloadFile\": ${4:20000}",
      "}"
    ],
    "description": "设置各种网络请求的超时时间。"
  }
}
